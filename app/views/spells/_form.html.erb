<%= render partial: "layouts/error", locals: {object: @spell} %>

<%= form_for spell, url:url do |f| %>
    <%= f.label "Name" %>
    <%= f.text_field :name %> <br>
    <%= f.label "Level" %> 
    <%= f.number_field :level %> <br>
    <%= f.label "Description" %> 
    <%= f.text_area :description %> <br>
    <%= f.hidden_field :spellbook_id %>
    <%= f.submit %>
<% end %>

